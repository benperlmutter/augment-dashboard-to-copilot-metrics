# Cookie Authentication
COOKIE_FILE=secrets/cookies.json

# API Configuration
# Replace with your actual dashboard/metrics API base URL
METRICS_API_BASE_URL=https://app.augmentcode.com/

# API Endpoints (adjust these to match your API structure)
USER_FEATURE_STATS_ENDPOINT=/api/user-feature-stats
TENANT_FEATURE_STATS_ENDPOINT=/api/tenant-feature-stats
TENANT_MAU_ENDPOINT=/api/tenant-monthly-active-users

# Which endpoints to scrape: "all", "user_stats", "tenant_stats", "tenant_mau", or comma-separated
SCRAPE_ENDPOINTS=user_stats

# Application Settings
LOOKBACK_DAYS=30
EXPORT_DIR=data
LOG_LEVEL=INFO

# Copilot Conversion Settings
# Your GitHub Enterprise ID for Copilot JSON conversion
ENTERPRISE_ID=283613

# HTTP Settings
REQUEST_TIMEOUT_SECONDS=30
MAX_RETRIES=3
RETRY_BACKOFF_SECONDS=0.5

